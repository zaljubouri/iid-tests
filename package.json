{
  "name": "iid-tests",
  "version": "1.0.4",
  "description": "A script to run eleven statistical tests which will determine whether a collection of random variables are independent and identically distributed (IID).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha",
    "lint": "eslint src/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaljubouri/iid-tests.git"
  },
  "keywords": [
    "iid",
    "nist",
    "random",
    "statistics",
    "independent",
    "identical",
    "distribute"
  ],
  "author": "Zaid Al-Jubouri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zaljubouri/iid-tests/issues"
  },
  "homepage": "https://github.com/zaljubouri/iid-tests#readme",
  "bin": {
    "iid-tests": "./dist/index.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@types/yargs": "^15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "global": "^4.4.0",
    "remove": "^0.1.5",
    "threads": "^1.6.3",
    "yargs": "^16.2.0"
  }
}
